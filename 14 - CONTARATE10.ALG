Algoritmo "Contarate10"

Var

  valor, salto, contador: inteiro

Inicio

contador <- 0
Escreval("Que valor você deseja?")
Leia(valor)
Escreval("Qual será o valor do salto?")
Leia(salto)
Enquanto (contador <= valor)faca
Escreval("O valor do contador é: ",contador)
contador <- contador + salto
FimEnquanto
     Escreval("Terminei de contar")
Fimalgoritmo